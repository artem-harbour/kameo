{
  "name": "@kameo/extension-form-radio-group",
  "description": "form radio group extension for kameo",
  "version": "0.0.20",
  "license": "MIT",
  "keywords": [
    "kameo",
    "kameo extension"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./radio-buttons": {
      "import": "./dist/radio-buttons/index.js",
      "require": "./dist/radio-buttons/index.cjs"
    },
    "./radio-options": {
      "import": "./dist/radio-options/index.js",
      "require": "./dist/radio-options/index.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@kameo/core": "^0.0.20",
    "lit": "3.3.0"
  },
  "peerDependencies": {
    "@kameo/core": "^0.0.20",
    "lit": "3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kameojs/kameo",
    "directory": "packages/extension-form-radio-group"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c"
  }
}
